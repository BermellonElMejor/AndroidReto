<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteX="-1dp"
    tools:layout_editor_absoluteY="37dp">

    <ImageButton
        android:id="@+id/boton_mapa"
        android:layout_width="53dp"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:layout_marginStart="64dp"
        android:layout_marginTop="48dp"
        android:adjustViewBounds="true"
        android:background="@color/transparente"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/mapa"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/lbl_calendario"
        style="@android:style/Widget.DeviceDefault.Light.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="20dp"
        android:fontFamily="sans-serif-black"
        android:fontStyle="normal"
        android:text="Calendario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_calendario" />

    <TextView
        android:id="@+id/lbl_distribuidores"
        style="@android:style/Widget.DeviceDefault.Light.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="24dp"
        android:fontFamily="sans-serif-black"
        android:fontStyle="normal"
        android:text="Distribuidores"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_delegaciones" />

    <TextView
        android:id="@+id/lbl_clientes"
        style="@android:style/Widget.DeviceDefault.Light.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="88dp"
        android:fontFamily="sans-serif-black"
        android:fontStyle="normal"
        android:text="Clientes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_partner" />

    <ImageButton
        android:id="@+id/boton_gmail"
        android:layout_width="58dp"
        android:layout_height="48dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="64dp"
        android:background="@color/transparente"
        android:contentDescription="Link_gmail"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/gmail" />

    <TextView
        android:id="@+id/lbl_pedidos"
        style="@android:style/Widget.DeviceDefault.Light.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="96dp"
        android:fontFamily="sans-serif-black"
        android:fontStyle="normal"
        android:text="Pedidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_pedidos" />

    <ImageButton
        android:id="@+id/boton_delegaciones"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="48dp"
        android:background="@color/transparente"
        android:contentDescription="boton_delegaciones"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_calendario"
        app:srcCompat="@drawable/delegaciones" />

    <ImageButton
        android:id="@+id/boton_partner"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="64dp"
        android:background="@color/transparente"
        android:contentDescription="boton_partner"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/boton_calendario"
        app:layout_constraintTop_toBottomOf="@+id/boton_telefono"
        app:srcCompat="@drawable/clientes" />

    <ImageButton
        android:id="@+id/boton_calendario"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="56dp"
        android:background="@color/transparente"
        android:contentDescription="boton_calendario"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_telefono"
        app:srcCompat="@drawable/calendario" />

    <ImageButton
        android:id="@+id/boton_pedidos"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="64dp"
        android:background="@color/transparente"
        android:contentDescription="boton_pedido"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/boton_delegaciones"
        app:layout_constraintTop_toBottomOf="@+id/lbl_clientes"
        app:srcCompat="@drawable/pedidos" />

    <ImageButton
        android:id="@+id/boton_telefono"
        android:layout_width="52dp"
        android:layout_height="48dp"
        android:layout_marginTop="48dp"
        android:background="@color/transparente"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/boton_gmail"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toEndOf="@+id/boton_mapa"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/telefono"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="176dp"
        android:layout_height="110dp"
        android:layout_marginTop="48dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gaming_grandma_logo"
        tools:srcCompat="@drawable/gaming_grandma_logo" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="100dp" />

</androidx.constraintlayout.widget.ConstraintLayout>